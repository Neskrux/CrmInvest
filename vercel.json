{
  "buildCommand": "cd frontend && npm install --force && CI=false DISABLE_ESLINT_PLUGIN=true npm run build-vercel",
  "outputDirectory": "frontend/build",
  "framework": null,
  "env": {
    "CI": "false",
    "DISABLE_ESLINT_PLUGIN": "true",
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "CI": "false",
      "DISABLE_ESLINT_PLUGIN": "true",
      "NODE_ENV": "production"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "regions": ["gru1"]
} 