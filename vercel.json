{
  "buildCommand": "cd frontend && npm install --force && CI=false DISABLE_ESLINT_PLUGIN=true npx react-scripts build",
  "outputDirectory": "frontend/build",
  "framework": null,
  "env": {
    "CI": "false",
    "DISABLE_ESLINT_PLUGIN": "true"
  },
  "build": {
    "env": {
      "CI": "false",
      "DISABLE_ESLINT_PLUGIN": "true"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
} 