    <div class="well">
      <h3>Enviar um Exemplo de Pedido</h3>
      <form class="form-horizontal">
        <fieldset>
          <div class="form-group">
            <label class="col-md-3 control-label" for="-sample-request-url">URL</label>
            <div class="input-group">
              <span class="input-group-addon">url</span>
              <input id="-sample-request-url" type="url" class="form-control sample-request-url" value="https://sandbox.idsf.com.br/api/banking/digitalaccount">
            </div>
          </div>

          <h3>Headers</h3>
            <div class="Baking-AddDigitalAccount-2_0_0-sample-request-header-fields">
              <div class="form-group">
                <label class="col-md-3 control-label" for="sample-request-header-field-Token">Token</label>
                <div class="input-group">
                  <span class="input-group-addon">String</span>
                  <input type="text" id="sample-request-header-field-Token" class="form-control sample-request-input" value="" placeholder="Token" data-family="header" data-name="Token" data-group="0">
                </div>
              </div>
            </div>

          <h3>Parameters</h3>
            <div class="col-md-3">
              <select name="Baking-AddDigitalAccount-2_0_0-sample-header-content-type" class="Baking-AddDigitalAccount-2_0_0-sample-request-param-select sample-header-content-type sample-header-content-type-switch">
                <option value="auto" selected="">ajax-auto</option>
                <option value="json">json</option>
                <option value="form-data">form-data</option>
              </select>
            </div>

