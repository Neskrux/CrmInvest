<script>
  function myFunction() {
    var logo = document.getElementsByClassName('logo')[0];
    console.log(logo.className)
    if(logo.className == 'logo'){
      logo.className = 'logo responsivo';
      document.getElementsByClassName('sidenav-search')[0].className = 'sidenav-search responsivo';
      //document.getElementsByClassName('sidenav-search-children')[0].style.top = '30px';
      document.getElementsByClassName('sidenav-search')[0].style.width = '70%';
      //document.getElementsByClassName('sidenav-search')[0].style.height = '100%';
      document.getElementsByClassName('sidenav-search')[0].style.zIndex = '1';
      document.getElementsByClassName('sidenav-search-children')[0].style.position = 'relative';
      document.getElementById('sidenav').children[0].children[1].className = 'sidenav nav nav-list list responsivo'; 
      document.getElementById('sidenav').style.position = 'relative'; 
      document.getElementById('sidenav').style.zIndex= '1'; 
      document.getElementsByClassName('form-control search')[0].style.marginLeft = '-10px';
      document.getElementsByClassName('form-control search')[0].style.width = '100%';
      //document.getElementsByClassName('search-reset')[0].style.width = '100px';
      document.getElementsByClassName('search-reset')[0].style.top = '102px';
      document.getElementsByClassName('menu-button-container')[0].style.marginLeft = '87%';
      document.getElementsByClassName('menu-button-container')[0].style.position = 'relative';
      document.getElementsByClassName('menu-button-container')[0].style.top = '-10px';
      //document.getElementsByClassName('search-reset')[0].style.right = '-5px';
      //document.getElementById('hamburgerBar').style.position = 'fixed';
      
      Array.prototype.forEach.call(document.getElementsByClassName('nav-header'), function(el){
          el.className = el.className + ' responsivo';
      });

      Array.prototype.forEach.call(document.getElementsByClassName('is-new'), function(el){
          el.className = el.className + ' responsivo';
      });
    }else{
      document.getElementsByClassName('menu-button-container')[0].style.marginLeft = '13%';
      logo.className = 'logo';
      document.getElementsByClassName('sidenav-search')[0].className = 'sidenav-search';
      document.getElementById('sidenav').children[0].children[1].className = 'sidenav nav nav-list list';

      Array.prototype.forEach.call(document.getElementsByClassName('nav-header'), function(el){
          el.className = el.className.replace('responsivo','');
      });

      Array.prototype.forEach.call(document.getElementsByClassName('is-new'), function(el){
          el.className = el.className.replace('responsivo','');
      });
    }
    
  }
  </script>
</head>

